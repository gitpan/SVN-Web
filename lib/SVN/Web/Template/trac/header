<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

[% IF at_head %]
[% revstr = '' %]
[% ELSE %]
[% revstr = "?rev=$rev" %]
[% END %]

<html>
  <head>
    <title>[% IF action == 'list' %][%|l%](repository list)[%END%][% ELSE %][% action %]: /[% repos %][% path %][% END %] ([% IF rev %]Rev: [% at_head ? 'HEAD' : rev %], [% END %][%|l%](via)[%END%] SVN::Web)</title>
    [% IF style %]
      <link rel="stylesheet" href="[% style %]" type="text/css" />
    [% ELSE %]
      <link rel="stylesheet" type="text/css" href="[% base_uri %]/css/trac/svnweb.css" />
    [% END %]
    [% IF action.match('^(?:browse|log|view)$') AND at_head %]
      <link rel="alternate" type="application/rss+xml"
        title="RSS Feed for [% path | uri | html %]"
        href="[% script %]/[% repos %]/rss[% path | uri | html %]" />
    [% END %]
  </head>
  <body>
    <div>
      [% UNLESS nonav %]
      <div id="navpath">
	<h1><a class="first" href="[% script %]/">[%|l%](repository list)[%END%]</a> /  [% url = [script,repos,''] %]  [% url = url.join('/') %]  [% urlpath = ['',''] %]<a href="[% url %]">[% repos %]</a>
            [% FOREACH p = navpaths %]
              [% CALL urlpath.splice(-1, 0, p) %]
              [% IF loop.count == loop.size %]
                [% IF p %]
                  <span class="sep">/</span>   [% p %]
                [% END %]
              [% ELSE %]
                <span class="sep">/</span>    <a href="[% url %]browse[% urlpath.join('/') | uri | html %][% revstr %]">[% p %]</a>
              [% END %]
          [% END %] @ [% at_head ? 'HEAD' : "r$rev" %]</h1>
      </div>
      [% END %]

      <div id="language-selection">
        <form action="[% self_uri %]">
          <select name="lang" onchange="this.form.submit();">
          [% FOREACH curlang IN languages -%]
            <option value="[% curlang.key %]"
            [%- IF curlang.key == lang %] selected="yes"[% END %]>[% curlang.value %]</option>
          [% END %]
          </select>
          [% FOREACH param IN cgi.param %]
            [% NEXT IF param == 'lang' %] 
            <input type="hidden" name="[% param %]" value="[% cgi.param(param) %]" />
          [% END %]
          <noscript>
            <input type="submit" value="Go" />
          </noscript>
        </form>
      </div>
    </div>

    <div id="content">
