    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

[% IF at_head %]
[% revstr = '' %]
[% ELSE %]
[% revstr = "?rev=$rev" %]
[% END %]

<html>
  <head>
    <title>[% IF action == 'list' %][%|l%](repository list)[%END%][% ELSE %][% action %]: /[% repos %][% path %][% END %] ([% IF rev %]Rev: [% at_head ? 'HEAD' : rev %], [% END %][%|l%](via)[%END%] SVN::Web)</title>
    [% IF style %]
      <link rel="stylesheet" href="[% style %]" type="text/css" />
    [% ELSE %]
      <link rel="stylesheet" type="text/css" href="[% base_uri %]/css/trac/svnweb.css" />
    [% END %]
  </head>
  <body>
    [% UNLESS nonav %]
      <div id="navpath">
	<h1><a class="first" href="[% script %]">[%|l%](repository list)[%END%]</a> /  [% url = [script,repos,''] %]  [% url = url.join('/') %]  [% urlpath = ['',''] %]<a href="[% url %]">[% repos %]</a>
            [% FOREACH p = navpaths %]
              [% CALL urlpath.splice(-1, 0, p) %]
              [% IF loop.count == loop.size %]
                [% IF p %]
                  <span class="sep">/</span>   [% p %]
                [% END %]
              [% ELSE %]
                <span class="sep">/</span>    <a href="[% url %]browse[% urlpath.join('/') | uri | html %][% revstr %]">[% p %]</a>
              [% END %]
          [% END %] @ [% at_head ? 'HEAD' : "r$rev" %]</h1>
      </div>
    [% END %]
    <div id="content">
